<Window x:Class="WinSeleccionDignidad"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Proyecto02PWPF"
        mc:Ignorable="d"
        Title="WinSeleccionDignidad" Height="300" Width="300" Icon="Images/check_2.ico">
    <Grid Background="#FFCEF0EE">
        <ComboBox x:Name="cmbDignidad" HorizontalAlignment="Left" Margin="85,144,0,0" VerticalAlignment="Top" Width="115" Height="23" FontWeight="Bold">
            <ComboBoxItem Content="Binomio" IsSelected="True"/>
            <ComboBoxItem Content="Alcalde"/>
            <ComboBoxItem Content="Concejal"/>
        </ComboBox>
        <Label x:Name="label" Content="Seleccione la dignidad deseada:" HorizontalAlignment="Left" Margin="51,102,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
        <Image x:Name="image" HorizontalAlignment="Left" Height="100" Margin="89,2,0,0" VerticalAlignment="Top" Width="100" Source="Images/usuario.png"/>
        <Button x:Name="btnSalir" Content="Salir" Margin="87,235,101,10" FontWeight="Bold" RenderTransformOrigin="0.505,1.4"/>

    </Grid>
</Window>
