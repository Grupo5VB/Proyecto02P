<Window x:Class="WinAsigDignidades"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VotoElectronico"
        mc:Ignorable="d"
        Title="WinAsigDignidades" Height="225.996" Width="344" Closing="Window_Closing">
    <Grid Background="#FFCEF0EE" Margin="0,0,0,-4">
        <Button x:Name="btn_nuevo" Content="Nuevo" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="54,152,0,0" FontWeight="Bold" FontSize="10"/>
        <Button x:Name="btn_regresar" Content="Regresar" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="237,152,0,0" FontWeight="Bold" FontSize="10"/>
        <Button x:Name="btn_guardar" Content="Guardar" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="145,152,0,0" FontWeight="Bold" FontSize="10"/>
        <Label x:Name="label" Content="Id:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="142,17,0,0" FontWeight="Bold" FontSize="10"/>
        <Label x:Name="label_Copy" Content="TipoDignidad:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="140,48,0,0" FontWeight="Bold" FontSize="10"/>
        <TextBox x:Name="txt_id" HorizontalAlignment="Left" Height="15" TextWrapping="Wrap" VerticalAlignment="Top" Width="99" Margin="218,20,0,0"/>
        <ComboBox x:Name="cbx_dignidad" HorizontalAlignment="Left" Margin="218,51,0,0" VerticalAlignment="Top" Width="99" FontSize="11" Height="19">
            <ComboBoxItem Content="Binomio" IsSelected="True"/>
            <ComboBoxItem Content="Alcalde"/>
            <ComboBoxItem Content="Concejal"/>
        </ComboBox>
        <Image x:Name="image" HorizontalAlignment="Left" Height="100" Margin="15,10,0,0" VerticalAlignment="Top" Width="100" Source="Images/dignidades.png"/>

    </Grid>
</Window>
